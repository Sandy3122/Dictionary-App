{
    "builds": [
      {
        "src": "/vite.config.js",
        "use": "@vercel/static-build",
        "config": {}
      }
    ],
    "routes": [
      {
        "src": "/(.*).(js|css|json|ico|png|svg|jpg|jpeg|gif|webp|mp4)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/src/$1.$2"
      },
      {
        "src": "/vite.config.js",
        "dest": "/vite.config.js"
      }
    ]
  }
  